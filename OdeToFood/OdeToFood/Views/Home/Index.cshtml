@using System.Collections
@model IEnumerable<OdeToFood.Data.Models.Restaurant>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Asp.Net</h1>
    @DateTime.Now.Year
    <div>
        There are @Model.Count() restaurants in the db.
    </div>
    <ul>
            @foreach (var restaurant in Model)
            {
                <li>@restaurant.Name</li>
            }
    </ul>
</div>
